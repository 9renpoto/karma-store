{
  "name": "karma-store-redis",
  "description": "simple karma-store",
  "version": "0.2.0",
  "author": "Keisuke Kan <@9renpoto>",
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "bugs": {
    "url": "https://github.com/9renpoto/karma-store/pulls"
  },
  "dependencies": {
    "cron": "^1.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "raven": "^2.1.1",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "^2.3.0",
    "@9renpoto/tslint-config": "^3.2.0",
    "@types/cron": "^1.2.1",
    "@types/faker": "^4.1.1",
    "@types/moment": "^2.13.0",
    "@types/redis": "^2.6.0",
    "ava": "^0.22.0",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-plugin-ava": "^4.2.1",
    "faker": "^4.1.0",
    "husky": "^0.14.0",
    "lint-staged": "^4.0.2",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.1.0",
    "prettier-eslint-cli": "^4.1.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/karma-store#readme",
  "keywords": [
    "karma",
    "redis"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier-eslint --write",
      "tslint --fix",
      "git add"
    ]
  },
  "main": "lib/store.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/karma-store.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc",
    "clean": "rimraf lib coverage .nyc_output",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t $CODECOV_TOKEN",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --cache",
    "lint:src": "tslint src/**/*.ts",
    "lint:test": "tslint test/**/*.ts",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "test": "nyc ava"
  }
}

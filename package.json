{
  "name": "karma-store-redis",
  "description": "simple karma-store",
  "version": "0.3.0",
  "author": "Keisuke Kan <@9renpoto>",
  "bugs": {
    "url": "https://github.com/9renpoto/karma-store/pulls"
  },
  "dependencies": {
    "@sentry/node": "4.4.1",
    "cron": "1.5.0",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "redis": "2.8.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.7.0",
    "@types/cron": "1.3.0",
    "@types/faker": "4.1.4",
    "@types/lodash": "4.14.118",
    "@types/mocha": "5.2.5",
    "@types/moment": "2.13.0",
    "@types/node": "9.6.39",
    "@types/power-assert": "1.5.0",
    "@types/redis": "2.8.8",
    "codecov": "3.1.0",
    "eslint": "5.9.0",
    "espower-typescript": "9.0.1",
    "faker": "4.1.0",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "power-assert": "1.6.1",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.1"
  },
  "directories": {
    "test": "test/"
  },
  "files": ["lib"],
  "homepage": "https://github.com/9renpoto/karma-store#readme",
  "keywords": ["karma", "redis"],
  "license": "MIT",
  "lint-staged": {
    "*.ts": ["prettier-eslint --write", "eslint --ext=ts", "git add"],
    "*.{md,json}": ["prettier --write", "git add"]
  },
  "main": "lib/store.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/karma-store.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib coverage .nyc_output",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint . --cache --ext=ts",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "test": "TS_NODE_TRANSPILE_ONLY=1 NODE_ENV=test nyc mocha -- test/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}

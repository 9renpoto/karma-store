{
  "name": "karma-store-redis",
  "description": "simple karma-store",
  "version": "0.2.0",
  "author": "Keisuke Kan <@9renpoto>",
  "bugs": {
    "url": "https://github.com/9renpoto/karma-store/pulls"
  },
  "dependencies": {
    "cron": "^1.3.0",
    "lodash": "^4.17.5",
    "moment": "^2.19.4",
    "raven": "^2.4.1",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.6.0",
    "@types/cron": "1.3.0",
    "@types/faker": "4.1.2",
    "@types/lodash": "4.14.104",
    "@types/mocha": "5.2.5",
    "@types/moment": "2.13.0",
    "@types/node": "9.4.6",
    "@types/power-assert": "1.4.29",
    "@types/raven": "2.1.5",
    "@types/redis": "2.8.7",
    "codecov": "3.1.0",
    "eslint": "5.8.0",
    "faker": "4.1.0",
    "husky": "1.1.3",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "8.0.4",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "power-assert": "1.4.4",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.3",
    "ts-node": "4.0.1",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "20.1.1"
  },
  "directories": {
    "test": "test/"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/karma-store#readme",
  "keywords": [
    "karma",
    "redis"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier-eslint --parser typescript --write",
      "eslint --ext=ts",
      "git add"
    ],
    "*.md": [
      "prettier --parser markdown --write",
      "git add"
    ]
  },
  "main": "lib/store.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/karma-store.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib coverage .nyc_output",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint . --cache --ext=ts",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "test": "nyc mocha -- test/*.test.ts"
  }
}

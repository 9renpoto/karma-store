{
  "name": "karma-store-redis",
  "description": "simple karma-store",
  "version": "0.2.0",
  "author": "Keisuke Kan <@9renpoto>",
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "bugs": {
    "url": "https://github.com/9renpoto/karma-store/pulls"
  },
  "dependencies": {
    "cron": "^1.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "raven": "^2.0.2",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "^0.3.0",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.0.0",
    "eslint-plugin-ava": "^4.2.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.51.0",
    "husky": "^0.14.0",
    "lint-staged": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "prettier-eslint-cli": "^4.1.1",
    "rimraf": "^2.6.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/karma-store#readme",
  "keywords": [
    "karma",
    "redis"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "main": "lib/store.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/karma-store.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d lib",
    "clean": "rimraf lib coverage .nyc_output",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t $CODECOV_TOKEN",
    "lint": "npm-run-all -p lint:*",
    "lint:es": "eslint . --cache",
    "lint:type": "flow",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "test": "npm-run-all -p test:*",
    "test:ava": "nyc ava",
    "test:lint": "npm run lint"
  }
}
